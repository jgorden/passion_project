  <% @batters.each do |batter| %>
    <tr>
      <td><a href='/players/batters/<%= batter.master.id %>'><%= batter.master.nameLast %></a></td>
      <td><%= batter.master.nameFirst %></td>
      <td><%= batter.teamID %></td>
      <td><%= batter.lgID %></td>
      <td><%= batter.g %></td>
      <td><%= batter.ab %></td>
      <td><%= batter.r %></td>
      <td><%= batter.h %></td>
      <td><%= batter.double %></td>
      <td><%= batter.triple %></td>
      <td><%= batter.hr %></td>
      <td><%= batter.rbi %></td>
      <td><%= batter.sb %></td>
      <td><%= batter.cs %></td>
      <td><%= batter.bb %></td>
      <td><%= batter.so %></td>
      <td><%= batter.ibb %></td>
      <td><%= batter.hbp %></td>
      <td><%= batter.sh %></td>
      <td><%= batter.sf %></td>
      <td><%= batter.gidp %></td>
    </tr>
