<table class='table'>
    <tr>
      <td>Last Name</td>
      <td>First Name</td>
      <td>Team</td>
      <td>lg</td>
      <td>G</td>
      <td>AB</td>
      <td>R</td>
      <td>H</td>
      <td>2b</td>
      <td>3b</td>
      <td>HR</td>
      <td>RBI</td>
      <td>SB</td>
      <td>CS</td>
      <td>BB</td>
      <td>SO</td>
      <td>iBB</td>
